<!DOCTYPE html>
<html>
<head>
  <title>JS Variable Practice 1</title>
</head>
<body>
  <h1>Open Console to See Output</h1>
  <script>
    var userName = "tharun";
    console.log("Using var:");
    console.log("Initial userName:", userName);

    userName = "y";
    console.log("Updated userName:", userName);
    let userAge = 20;
    console.log("\nUsing let:");
    console.log("Initial userAge:", userAge);

    userAge = 21;
    console.log("Updated userAge:", userAge);
    const userCountry = "India";
    console.log("\nUsing const:");
    console.log("userCountry:", userCountry);

    console.log("\nâœ” Program finished successfully!");
  </script>
</body>
</html>
